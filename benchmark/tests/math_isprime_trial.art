prime: $(n){
    if n=2 { return true }
    if n=3 { return true }

    if [or n<=1 n%2=0] { return false }
    
    upper: sqrt n
    i: 3
    loop i<upper {
        if n%&=0 { return false }
        i: i+2
    }
    return true
}

maxLimit: 10000

i: 0
loop i<maxLimit-1 {
	print [prime i]
	i: i+1
}