bubbleSort: [items]{
    changed: true
    loop changed {
        changed: false
        i: 0
        loop $(size items)-1 {
            if items.$(i)>items.$(i+1) {
                swap items i i+1
                changed: true
            }
            i: i+1
        }
    }
    items
}

list: shuffle|range 1 2000
list: bubbleSort list
loop list {
    print &
}
