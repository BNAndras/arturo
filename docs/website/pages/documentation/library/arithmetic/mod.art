name: "mod"
type: :function
module: "Arithmetic"
returns: [
        :integer
         :nothing
    ]
alias: "%"
infix?: true
description: "calculate the modulo of given values and return result"
examples: [
        {
            print mod 5 2      ; 1
            print 9 % 3        ; 0
        }
         {
            a: 8
            mod 'a 3           ; a: 2
        }
    ]
attributes: #[
    ]
arguments: #[
    valueA: [
            :integer
             :literal
        ]
    valueB: [
            :integer
        ]
    ]
related: [
        "Arithmetic/mul"
         "Arithmetic/pow"
         "Arithmetic/add"
         "Binary/and"
    ]