name: "neg"
type: :function
module: "Arithmetic"
description: "reverse sign of given value and return it"
returns: [
        :integer
         :floating
         :complex
         :nothing
    ]
examples: [
        "print neg 1        ; -1"
         {
            a: 5
            neg 'a             ; a: -5
        }
    ]
attributes: #[
    ]
arguments: #[
    value: [
            :integer
             :floating
             :complex
             :literal
        ]
    ]
related: [
        "Core/new"
         "Arithmetic/dec"
         "Binary/nor"
         "Binary/not"
    ]