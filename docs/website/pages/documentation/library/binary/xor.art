name: "xor"
type: :function
module: "Binary"
description: "calculate the binary XOR for the given values"
returns: [
        :integer
         :nothing
    ]
examples: [
        "print xor 2 3      ; 1"
         {
            a: 2
            xor 'a 3           ; a: 1
        }
    ]
attributes: #[
    ]
arguments: #[
    valueA: [
            :integer
             :literal
        ]
    valueB: [
            :integer
        ]
    ]
related: [
        "Binary/nor"
         "Binary/or"
         "Binary/xnor"
         "Binary/and"
    ]