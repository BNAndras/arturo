name: "insert"
type: :function
module: "Collections"
description: "insert value in collection at given index"
returns: [
        :string
         :dictionary
         :block
         :nothing
    ]
examples: [
        {
            insert [1 2 3 4] 0 "zero"
            ; => ["zero" 1 2 3 4]
            
            print insert "heo" 2 "ll"
            ; hello
        }
         {
            dict: #[
                name: John
            ]
            
            insert 'dict 'name "Jane"
            ; dict: [name: "Jane"]
        }
    ]
attributes: #[
    ]
arguments: #[
    collection: [
            :string
             :literal
             :dictionary
             :block
        ]
    index: [
            :integer
             :string
        ]
    value: [
            :any
        ]
    ]
related: [
        "Colors/invert"
         "Reflection/inspect"
         "Strings/indent"
         "Collections/index"
    ]