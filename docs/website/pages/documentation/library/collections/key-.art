name: "key?"
address: "0x101936C78"
type: :function
module: "Collections"
returns: [
        :boolean
    ]
description: "check if dictionary contains given key"
example: {
        user: #[
            name: "John"
            surname: "Doe"
        ]
        
        key? user 'age            ; => false
        if key? user 'name [
            print ["Hello" user\name]
        ]
        ; Hello John
    }
