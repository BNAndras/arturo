name: "key?"
type: :function
module: "Collections"
description: "check if dictionary contains given key"
returns: [
        :logical
    ]
examples: [
        {
            user: #[
                name: "John"
                surname: "Doe"
            ]
            
            key? user 'age            ; => false
            if key? user 'name [
                print ["Hello" user\name]
            ]
            ; Hello John
        }
    ]
attributes: #[
    ]
arguments: #[
    collection: [
            :dictionary
        ]
    key: [
            :any
        ]
    ]
related: [
        "Collections/keys"
         "Core/try?"
         "Logic/any?"
         "Reflection/set?"
    ]