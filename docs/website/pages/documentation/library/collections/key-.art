name: "key?"
type: :function
description: "check if collection contains given key"
module: "Collections"
returns: [
                :logical
        ]
line: 2832
source: "https://github.com/arturo-lang/arturo/blob/v0.9.83/src/library/Collections.nim#L2832"
examples: [
                {
                        user: #[
                            name: "John"
                            surname: "Doe"
                        ]
                        
                        key? user 'age            ; => false
                        if key? user 'name [
                            print ["Hello" user\name]
                        ]
                        ; Hello John
                }
        ]
attributes: #[
        ]
arguments: #[
        collection: [
                        :dictionary
                         :object
                ]
        key: [
                        :any
                ]
        ]
related: [
                "Collections/empty?"
                 "Collections/contains?"
                 "Collections/one?"
                 "Collections/in?"
        ]