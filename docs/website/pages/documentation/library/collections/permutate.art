name: "permutate"
type: :function
module: "Collections"
description: "get all possible permutations of the elements in given collection"
returns: [
        :block
    ]
examples: [
        {
            permutate [A B C]
            ; => [[A B C] [A C B] [C A B] [B A C] [B C A] [C B A]]
        }
    ]
attributes: #[
    ]
arguments: #[
    collection: [
            :block
        ]
    ]
related: [
        "System/terminate"
         "Colors/desaturate"
         "Colors/palette"
         "Colors/saturate"
    ]