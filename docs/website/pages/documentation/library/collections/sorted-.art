module: "Collections"
returns: [
                :logical
        ]
line: 2919
source: "https://github.com/arturo-lang/arturo/blob/v0.9.83/src/library/Collections.nim#L2919"
examples: [
                {
                        sorted? [1 2 3 4 5]         ; => true
                        sorted? [4 3 2 1 5]         ; => false
