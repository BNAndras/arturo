name: "desaturate"
type: :function
module: "Colors"
returns: [
        :color
    ]
description: "desaturate color by given percentage (0.0-1.0)"
examples: [
        {
            desaturate #red 0.2         ; => #E61919
            desaturate #red 0.5         ; => #BF4040
            
            desaturate #9944CC 0.3      ; => #9558B8
        }
    ]
attributes: #[
    ]
arguments: #[
    color: [
            :color
        ]
    percent: [
            :floating
        ]
    ]
related: [
        "Colors/saturate"
         "Colors/darken"
         "Colors/lighten"
         "Collections/permutate"
    ]