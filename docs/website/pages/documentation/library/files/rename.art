name: "rename"
type: :function
module: "Files"
returns: [
        :nothing
    ]
description: "rename file at path using given new path name"
examples: [
        {
            rename "README.md" "READIT.md"
            ; file renamed
        }
    ]
attributes: #[
    directory: #[
        types: [
                :boolean
            ]
        description: "path is a directory"
        ]
    ]
arguments: #[
    file: [
            :string
        ]
    name: [
            :string
        ]
    ]
related: [
        "Collections/remove"
         "Files/read"
         "Numbers/range"
         "Strings/render"
    ]