name: "or?"
type: :function
module: "Logic"
returns: [
        :logical
    ]
description: "return the logical OR for the given values"
examples: [
        {
            x: 2
            y: 4
            
            if or? x=2 y>5 [
                print "yep, that's correct!"]
            ]
            
            ; yep, that's correct!
        }
    ]
attributes: #[
    ]
arguments: #[
    valueA: [
            :logical
             :block
        ]
    valueB: [
            :logical
             :block
        ]
    ]
related: [
        "Logic/nor?"
         "Logic/xor?"
         "Logic/xnor?"
         "Binary/or"
    ]