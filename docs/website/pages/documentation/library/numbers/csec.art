name: "csec"
type: :function
module: "Numbers"
returns: [
        :floating
         :complex
    ]
description: "calculate the cosecant of given angle"
examples: [
        {
            print csec 0                ; inf
            print csec 0.3              ; 3.383863361824123
            print csec 1.0              ; 1.188395105778121
        }
         {
            csec to :complex [1 1]  
            ; => 0.6215180171704283-0.3039310016284264i
        }
    ]
attributes: #[
    ]
arguments: #[
    angle: [
            :integer
             :floating
             :complex
        ]
    ]
related: [
        "Numbers/sec"
         "Numbers/acsec"
         "Numbers/asec"
         "Numbers/csech"
    ]