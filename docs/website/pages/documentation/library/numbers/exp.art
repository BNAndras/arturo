name: "exp"
type: :function
module: "Numbers"
returns: [
        :floating
         :complex
    ]
description: "calculate the exponential function for given value"
example: {
        print exp 1.0           ; 2.718281828459045
        print exp 0             ; 1.0
        print exp neg 1.0       ; 0.3678794411714423
        
        exp to :complex @[pi 1]
        ; => 12.50296958887651+19.47222141884161i
    }
attributes: #[
    ]
arguments: #[
    value: [
            :integer
             :floating
             :complex
        ]
    ]
related: [
        "Core/dup"
         "Core/pop"
         "Files/zip"
         "Iterators/map"
    ]