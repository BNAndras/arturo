name: "ln"
type: :function
module: "Numbers"
returns: [
        :floating
         :complex
    ]
description: "calculate the natural logarithm of given value"
example: {
        print ln 1.0            ; 0.0
        print ln 0              ; -inf
        print ln neg 7.0        ; nan
    }
