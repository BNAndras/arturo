name: "send?"
type: :function
description: "send given message to selected socket and return true if successful"
module: "Sockets"
returns: [
                :logical
        ]
line: 272
source: "https://github.com/arturo-lang/arturo/blob/v0.9.83/src/library/Sockets.nim#L272"
examples: [
                {
                        ; connect to a local server on port 256
                        socket: connect.to:"localhost" 256
                        
                        ; send a message to the server
                        ; and check if it was successful
                        sent?: send? socket "Hello Socket World"
                        
                        print ["Message was sent successfully:" sent?]
                }
        ]
attributes: #[
        ]
arguments: #[
        destination: [
                        :socket
                ]
        message: [
                        :string
                ]
        ]
related: [
                "Sockets/send"
                 "Sockets/listen"
                 "Sockets/connect"
                 "Sets/superset?"
        ]