name: "capitalize"
type: :function
module: "Strings"
description: "convert given string to capitalized"
returns: [
        :string
         :nothing
    ]
examples: [
        {
            print capitalize "hello World"      ; "Hello World"
        }
         {
            str: "hello World"
            capitalize 'str                     ; str: "Hello World"
        }
    ]
attributes: #[
    ]
arguments: #[
    string: [
            :string
             :literal
        ]
    ]
related: [
        "Strings/upper"
         "Strings/lower"
         "Strings/strip"
         "Strings/truncate"
    ]