name: "whitespace?"
type: :function
module: "Strings"
returns: [
        :boolean
    ]
description: "check if given string consists only of whitespace"
example: {
        whitespace? "hello"           ; => false
        whitespace? " "               ; => true
        whitespace? "\n \n"           ; => true
    }
attributes: #[
    ]
arguments: #[
    string: [
            :string
        ]
    ]
related: [
        "Reflection/literal?"
         "Strings/lower?"
         "Core/when?"
         "Core/while"
    ]