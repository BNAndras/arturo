name: "whitespace?"
type: :function
module: "Strings"
description: "check if given string consists only of whitespace"
returns: [
        :logical
    ]
examples: [
        {
            whitespace? "hello"           ; => false
            whitespace? " "               ; => true
            whitespace? "\n \n"           ; => true
        }
    ]
attributes: #[
    ]
arguments: #[
    string: [
            :string
        ]
    ]
related: [
        "Reflection/literal?"
         "Strings/lower?"
         "Comparison/same?"
         "Core/when?"
    ]