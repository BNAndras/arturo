name: "terminate"
type: :function
module: "System"
description: "kill process with given id"
returns: [
        :nothing
    ]
examples: [
        {
            ; start process
                pid: execute.async "someProcessThatDoesSomethingInTheBackground"
            
                ; wait for 5 seconds
                pause 5000 
            
                ; terminate background process
                terminate pid
        }
    ]
attributes: #[
    code: #[
        types: [
                :integer
            ]
        description: "use given error code"
        ]
    ]
arguments: #[
    id: [
            :integer
        ]
    ]
related: [
        "Io/terminal"
         "Collections/permutate"
         "Strings/truncate"
         "Converters/define"
    ]