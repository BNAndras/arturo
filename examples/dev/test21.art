"=========================="
"Fibonacci calculator"
"=========================="

// Get limit from command line
maxLimit $(to.number @0)

// Define our recursive function
fib { 
	if (@0<2) { 1 } { 
		$(fib: @0-1) + $(fib: @0-2) 
	} 
}

// Calculate fib(x) for x:1->maxLimit
foreach $(range 1 maxLimit) { 
	print "fib(" + @ + ") = " + $(fib: @) 
}
