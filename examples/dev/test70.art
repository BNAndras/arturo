fibby [n]{
	//print ~HERE
	fibs #(0)*100
	//print fibs
	fibs.1 1
	//print fibs
	fibs.2 1
	//print fibs
	loop $(range 3 n) [i]{
		fibs.[i] fibs.[i-1]+fibs.[i-2]
	}
	//print fibs
	return fibs.[n]
}

loop $(range 1 20) {
	print $(!fibby @)
}
