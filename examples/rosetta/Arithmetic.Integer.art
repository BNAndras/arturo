print "give me the first number  : " true, 	a $(toNumber|strip|input)
print "give me the second number : " true, 	b $(toNumber|strip|input)

print "`a` + `b` = " + (a+b)
print "`a` - `b` = " + (a-b)
print "`a` * `b` = " + (a*b)
print "`a` / `b` = " + (a/b)
print "`a` % `b` = " + (a%b)
print "`a` ^ `b` = " + (a^b)
