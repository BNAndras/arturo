// set some json string
json: "{ \"foo\": 1, \"bar\": [10, \"apples\"] }"

// parsing json string to object
print "JSON:"
inspect [parseJson json]

// set some object
object: #{
	name: "john"
	surname: "doe"
	address: #{
		number: 10
		street: "unknown"
		country: "Spain"
	}
	married: false
}

// generate json string from object
print "OBJECT:"
print [generateJson object]
