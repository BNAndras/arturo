prime: @(n){
    if n=2 { return true }
    if n=3 { return true }
    if [or n<=1 n%2=0] { return false }
    
    upper: [toNumber [sqrt [toReal n]]]
    loop [rangeBy 3 upper 2] {
        if n%&=0 { return false}
    }
    return true
}

loop 1..20 {
    print "isPrime(" + & + ")? = " + [prime &]
}