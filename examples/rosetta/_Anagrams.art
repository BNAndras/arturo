text: download "http://wiki.puzzlers.org/pub/wordlists/unixdict.txt"
anagrams: #{}
cnt: 0

loop [lines text] @(line){
    print line
    word: strip line
    key: join [sort|chars word] ""
    if [not|hasKey anagrams key] { anagrams.[key]: #(word) } { anagrams.[key]: anagrams.[key] + word }

    cnt: max #(cnt [size anagrams.[key]])
}

loop anagrams @(k v){
    if [size v]=cnt -> print [join v " "]
}
