define :queue [
    init: function [][
        this\items: []
    ]

    empty?: function [][
        zero? this\items
    ]

    push: function [item][
        this\items: this\items ++ item
    ]

    pop: function [][
        ensure -> [not? this\empty?]
        
        result: this\items\0
        this\items: remove.index this\items 0
        return result
    ]
]