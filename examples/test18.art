str "this is a string variable"		// a string variable
num 10 								// a number variable

arrA 1 2 3 							// an array
arrB #(1 2 3)						// also an array
arrC #("one" 2 "three") 			// another array

dict #{								// defining a dictionary
	name "john"
	surname "zafeiropoulos"
	age 33
}

func {
	print "this is a function"
}

class #{ 								// this is a class
	prop "this is a class variable"
	sayHello {
		// and this is a class method
		return "hello " + ~.0
	}
}

// And a couple of stuff about how this
// whole thing really works

print $(class.sayHello: "X")
print $(map arrA { return ~.0 * 2 })
print $(filter arrB { return (~.0 % 2 == 0) })

