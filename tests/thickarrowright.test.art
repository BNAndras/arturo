; WITH SIMPLE VALUE

print map 1..10 => 5
print map 1..3 => "a"

; WITH WORD

print select 1..10 => even?
print filter 1..10 => odd?

; WITH USER FUNCTION

double: function [x][2*x]

print map 1..10 => double

; WITH INLINE

print map 1..10 => (5)

; WITH INLINE + REPLACEMENT

print map 1..10 => (_+2)
print map 5..10 => (_-2)
print map 1..10 => (2-_)
